{
  "name": "memimgview",
  "displayName": "MemImgView",
  "description": "View images in memory while debugging.",
  "repository": "https://github.com/xziyue/MemImgView",
  "version": "1.0.0",
  "publisher": "xziyue",
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Debuggers"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "memimgview.openPanel",
        "title": "Show MemImgView Panel"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.97.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "@vscode/vsce": "^3.6.0",
    "eslint": "^9.23.0",
    "typescript": "^5.8.2"
  }
}
